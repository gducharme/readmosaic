{
  "artifact_producers": {
    "items.jsonl": "ingest",
    "manifest.json": "publish",
    "reviewed.jsonl": "future_review",
    "transformed.jsonl": "transform"
  },
  "determinism_policy": "strict",
  "item_unit": "item",
  "pipeline_id": "example-pipeline",
  "stages": [
    {
      "inputs": [],
      "mode": "whole_run",
      "outputs": [
        "items.jsonl"
      ],
      "placeholder": false,
      "stage_id": "ingest"
    },
    {
      "inputs": [
        "items.jsonl"
      ],
      "mode": "per_item",
      "outputs": [
        "transformed.jsonl"
      ],
      "placeholder": false,
      "stage_id": "transform"
    },
    {
      "inputs": [
        "transformed.jsonl"
      ],
      "mode": "whole_run",
      "outputs": [
        "reviewed.jsonl"
      ],
      "placeholder": true,
      "stage_id": "future_review"
    },
    {
      "inputs": [
        "reviewed.jsonl"
      ],
      "mode": "whole_run",
      "outputs": [
        "manifest.json"
      ],
      "placeholder": false,
      "stage_id": "publish"
    }
  ]
}
